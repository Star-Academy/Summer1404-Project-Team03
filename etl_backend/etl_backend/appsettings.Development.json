{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Keycloak": {
    "AuthorizationUrl": "http://localhost:8080/realms/codestar/protocol/openid-connect/auth",
    "AuthServerUrl": "http://localhost:8080",
    "Realm": "codestar",
    "ClientId": "my_backend_app",
    "Audience": "account",
    "AccessCookieName": "access_token",
    "RefreshCookieName": "refresh_token",
    "ClockSkewSeconds": 30,
    "ExpClaimType": "exp",
    "RoleScope": "realm_access",
    "RolesKey": "roles"
  },
  "Authentication": {
    "MetadataAddress": "http://localhost:8080/realms/codestar/.well-known/openid-configuration",
    "ValidIssuer": "http://localhost:8080/realms/codestar",
    "Audience": "account",
    "ClientId": "my_backend_app",
    "Authority": "http://localhost:8080"
  },
  "Authorization": {
    "Roles": {
      "DataAdmin": "data_admin",
      "SysAdmin": "sys_admin",
      "Analyst": "data_analyst"
    }
  }
}
